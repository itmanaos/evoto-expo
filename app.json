{
  "expo": {
    "name": "evoto-expo",
    "slug": "evoto-expo",
    "scheme": "evoto-expo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "src/assets/ic-evoto.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "src/assets/splash-1080-2160.png",
      "resizeMode": "contain",
      "backgroundColor": "#D9D9D9"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.itmanaos.evotoexpo"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "src/assets/adaptive-icon.png",
        "backgroundColor": "#e1e1e1"
      },
      "package": "com.itmanaos.evotoexpo",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBTdFMUZglGu5MGpzf40GXlJm-AvckiyAQ"
        }
      }
    },
    "plugins": ["expo-router", "expo-secure-store"],
    "extra": {
      "router": {
        "origin": false
      },
      "experiments": {
        "typedRoutes": true
      },
      "eas": {
        "projectId": "f03cba0e-ce4a-4087-a0a9-54cc524ff40a"
      }
    }
  }
}
